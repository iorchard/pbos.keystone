[DEFAULT]
log_file = {{ os_service_name }}.log
log_dir = /var/log/{{ os_service_name }}
transport_url = rabbit://openstack:{{ rabbitmq_openstack_password }}@{{ keepalived_vip }}:{{ rabbitmq_tls_port }}
[application_credential]
[assignment]
[auth]
[cache]
memcache_servers = {{ keepalived_vip }}:{{ memcached_port }}
[catalog]
template_file = /etc/keystone/default_catalog.templates
[cors]
[credential]
[database]
connection = mysql+pymysql://{{ os_service_name }}:{{ db_password }}@{{ keepalived_vip }}/{{ os_service_name }}?charset=utf8&ssl_ca={{ mariadb_ca_cert }}
[domain_config]
[endpoint_filter]
[endpoint_policy]
[eventlet_server]
[federation]
sso_callback_template = /etc/keystone/sso_callback_template.html
[fernet_receipts]
[fernet_tokens]
[healthcheck]
[identity]
[identity_mapping]
[jwt_tokens]
[ldap]
[memcache]
[oauth1]
[oslo_messaging_amqp]
[oslo_messaging_kafka]
[oslo_messaging_notifications]
driver = messagingv2
[oslo_messaging_rabbit]
ssl = true
ssl_version = TLSv1_2
ssl_ca_file = {{ rabbitmq_ca_cert }}
rabbit_retry_interval={{ rabbit_retry_interval }}
rabbit_retry_backoff={{ rabbit_retry_backoff }}
rabbit_interval_max={{ rabbit_interval_max }}
rabbit_quorum_queue={{ rabbit_quorum_queue }}
heartbeat_timeout_threshold={{ heartbeat_timeout_threshold }}
heartbeat_rate={{ heartbeat_rate }}

[oslo_middleware]
[oslo_policy]
policy_dirs = /etc/keystone/policy.d
[policy]
[profiler]
[receipt]
[resource]
[revoke]
[role]
[saml]
[security_compliance]
[shadow_users]
[token]
provider = fernet
[tokenless_auth]
[totp]
[trust]
[unified_limit]
[wsgi]
